{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
        "data": {
        "url": "https://api.allorigins.win/raw?url=https://api.ons.gov.uk/timeseries/i46d/dataset/lms/data",
        "format": {
          "type":"json",
          "property": "months"}},
  
  
       "mark":{
        "type": "line",
        "color":"red",
        "strokeWidth":1,
        "opacity":1},
  
      "title": {
      "text": ["Economic Inactivity rate"],
      "subtitle": ["UK: People: Aged 16-64: %"],
      "limit": 250,
      "subtitleFontStyle":"italic",
      "subtitleFontSize":10,
      "anchor": "start",
      "color": "black"},
  
      "width":200,
      "height":200,
  
      "encoding": {
        "x": {
          "field": "date",
          "type": "temporal",
          "title":null,
          "axis": {"grid": false, "ticks": false}
          },
  
        "y": {
          "field": "value",
          "type": "quantitative",
          "title":null,
          "axis": {"grid": false, "ticks": false}
          }
          
  
        
      }
  }